    "@storybook/react": "^6.1.0-beta.4",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "babel-loader": "^8.1.0",
    "date-fns": "^2.15.0",
    "husky": "^4.2.5",
    "microbundle": "^0.12.4",
    "mutation-observer": "^1.0.3",
    "react": "^17.0.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^17.0.1",
